{
    "name": "gdevilbat/sparda-cms-core-module",
    "type": "laravel-module",
    "description": "",
    "authors": [
        {
            "name": "M.Aldi Nur Ibrahim",
            "email": "maldinuribrahim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.75",
        "intervention/image": "^2.5",
        "alexusmai/laravel-file-manager": "^2.0",
        "doctrine/dbal": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gdevilbat\\SpardaCMS\\Modules\\Core\\Providers\\CoreServiceProvider"
            ],
            "aliases": {
                
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Gdevilbat\\SpardaCMS\\Modules\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/preprocessor/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Gdevilbat\\SpardaCMS\\Modules\\Core\\Tests\\": "Tests"
        }
    },
    "archive": {
        "exclude": ["/Tests/Browser", "/Tests/Feature", "/preprocessor"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
